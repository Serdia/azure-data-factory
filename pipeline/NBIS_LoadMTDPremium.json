{
	"name": "NBIS_LoadMTDPremium",
	"properties": {
		"description": "Load data from excel to sql",
		"activities": [
			{
				"name": "fromExcelToSql",
				"description": "copy data from excel file located in blob to sql database",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "FileServerReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "dsExcelOnPrem",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}